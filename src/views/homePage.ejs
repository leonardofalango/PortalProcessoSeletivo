    </div>

    <div class="main">
        <table class="container bordered centered highlight responsive-table">
            <tr>
                <td><b>Data</b></td>
                <td><b>Nome</b></td>
                <td><b>Vagas</b></td>
                <td><b>Link</b></td>
            </tr>

            <% for (let i = 0; i < preocessData.length; i++) { %>
                <% for (let j = 0; j < jobData.lenght; j++) { %>

                    <% if (processData[i].FK_job == jobData[j].id) { %> 
                        <td><%= processData[i].date%></td> 
                        <td><%= jobData[j].name %></td> 
                        <td><%= processData.capacity%></td> 
                        <td>Link</td> 
                    <% } %> 

                <% } %> 
            <% } %> 

